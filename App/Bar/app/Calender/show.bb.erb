<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @calender.title %></h1>
    <%= link_to "Edit", :action => :edit, :id => @calender.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Title</td>
        <td class="itemValue"><%= @calender.title%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Message</td>
        <td class="itemValue"><%= @calender.message%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Date</td>
        <td class="itemValue"><%= @calender.date%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Time</td>
        <td class="itemValue"><%= @calender.time%></td>
      </tr>
      
    </table>
  </div>

</div>
