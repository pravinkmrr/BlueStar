<div data-role="page" style="background:#333">

  <div data-role="header" data-position="inline">
    <h1>Events</h1>
    <a  href="#" class="ui-btn-left" data-icon="back" data-rel='back'  data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
      Back
    </a>
      <img src="<%= Rho::RhoApplication::get_blob_path($right_image) %>" class="ui-btn-right" />        
  </div>
     
        <div data-role="header" data-position="inline">
             <input type="search" >
        </div>
  <div data-role="content" class="bg home_content">
    <div class="grids">
    <% i = 0 %>

    <% @eicons.each do |event|  %>
      <% i+=1 %>
      <% if i===1 %>  
      <div class="ui-grid-b top">
            <div class="ui-block-a">
                <a href="<%= url_for :controller =>:Welcome, :action => :index  %>">
                     <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                </a>
           </div>
    <% end %>
    <% if i===2 %>    
    <div class="ui-block-b">
                   <a href="<%= url_for :controller =>:Venue, :action => :index %>">
                        <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                   </a>
              </div>
        <% end %>
        <% if i===3 %>    
            <div class="ui-block-c">
                           <a href="<%= url_for :controller =>:Guide, :action => :index %>">
                                <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                           </a>
                      </div>
      </div>            
          <% end %>      
          
      <% if i===4 %>  
      <div class="ui-grid-b middle">
            <div class="ui-block-a">
                <a href="<%= url_for :controller =>:Exhibitor, :action => :index  %>">
                     <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                </a>
           </div>
    <% end %>
    <% if i===5 %>    
    <div class="ui-block-b">
                   <a href="<%= url_for :controller =>:Agenda, :action => :index %>">
                        <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                   </a>
              </div>
        <% end %>
        <% if i===6 %>    
            <div class="ui-block-c">
                           <a href="<%= url_for :controller =>:Map, :action => :index %>">
                                <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                           </a>
                      </div>
</div>
          <% end %>      

<% if i===7 %>  
<div class="ui-grid-b bottom">

      <div class="ui-block-a">
          <a href="<%= url_for :controller =>:Calender, :action => :index  %>">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
          </a>
     </div>
<% end %>
<% if i===8 %>    
<div class="ui-block-b">
             <a href="<%= url_for :controller =>:Keynote, :action => :index %>">
                  <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
             </a>
        </div>
  <% end %>
  <% if i===9 %>    
      <div class="ui-block-c">
                     <a href="<%= url_for :controller =>:Workshop, :action => :index %>">
                          <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" >
                     </a>
                </div>
    </div>
    <% end %>               
    <% end %>
    </div>
    </div>
  <div class="ad">
      <% if $ad_image!="" %>
<a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open, :query=> {:link => $website_link} %>').success(function(data){console.log('PDF')}); return false;"> 
      <img src="<%= Rho::RhoApplication::get_blob_path($ad_image) %>" />
      </a>
      <% end %>
   </div>
</div>
