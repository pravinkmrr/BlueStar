<div data-role="page">

	<div data-role="header" data-position="inline">
    <h1>Workshops</h1>
<a href="#" data-rel="back" class="ui-btn-left" data-icon="back" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
  Back
</a>
<img class="ui-btn-right" src="<%= Rho::RhoApplication::get_blob_path($right_image) %>" />
	</div>

  <div data-role="header" data-position="inline">
       <input type="search" >
  </div>
  <div data-role="content">
  <% 
  by_type = []
  @presentations.each do |h,i|
    puts "PRESSSSSSS#{h.workshop_id}"
    by_type.push(h.workshop_id)
  end
  %>
    <ul data-role="listview">
      <% @workshops.each do |workshop| %>      
        <% if by_type.include?(workshop.object) %>
        <li>
            <a href="<%= url_for :controller => :Presentation, :action => :index, :query=>{:wid => workshop.object} %>">
              <%= workshop.name %>
            </a>
          </li>     
      <% end %> 
      <% end %>
    </ul>
  </div>

</div>
