<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @advertisement.image %></h1>
    <%= link_to "Edit", :action => :edit, :id => @advertisement.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Image</td>
        <td class="itemValue"><%= @advertisement.image%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Filename</td>
        <td class="itemValue"><%= @advertisement.filename%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EventId</td>
        <td class="itemValue"><%= @advertisement.event_id%></td>
      </tr>
      
    </table>
  </div>

</div>
