<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @advertisement.image %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @advertisement.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">Image</div>
            <div class="itemValue"><%= @advertisement.image %></div>
          </li>
      
          <li>
            <div class="itemLabel">Filename</div>
            <div class="itemValue"><%= @advertisement.filename %></div>
          </li>
      
          <li>
            <div class="itemLabel">EventId</div>
            <div class="itemValue"><%= @advertisement.event_id %></div>
          </li>
      
    </ul>
  </div>

</div>
