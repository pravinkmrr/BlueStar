<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @link.links_url %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @link.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">LinksUrl</div>
            <div class="itemValue"><%= @link.links_url %></div>
          </li>
      
          <li>
            <div class="itemLabel">RegionId</div>
            <div class="itemValue"><%= @link.region_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">TypeId</div>
            <div class="itemValue"><%= @link.type_id %></div>
          </li>
      
    </ul>
  </div>

</div>
