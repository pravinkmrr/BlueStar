<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @icon.icon_name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @icon.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">IconName</td>
        <td class="itemValue"><%= @icon.icon_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">IconFile</td>
        <td class="itemValue"><%= @icon.icon_file%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Icon</td>
        <td class="itemValue"><%= @icon.icon%></td>
      </tr>
      
    </table>
  </div>

</div>
