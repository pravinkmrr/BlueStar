<div data-role="page" style="background:#333">
   <div data-role="header" data-position="inline">
      <h1>Events</h1>
      <a href="<%= Rho::RhoConfig.start_path %>" class="ui-btn-left" data-icon="back" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
      Back
      </a>
      <img src="/public/images/logo.png" class="ui-btn-right" />        
   </div>
   <div data-role="header" data-position="inline">
        <input type="search" >
   </div>
   <div data-role="content" class="bg home_content">
      <div class="grids">
         <% if @icons.any? %>
         <% i = 0 %>
         <% @icons.each do |event|  %>
         <% i+=1 %>  
         <% if i===1 %>
         <div class="ui-grid-b top">
            <div class="ui-block-a">
               <% if @events.any? %>
               <a href="<%= url_for :controller =>:Event, :action => :index %>">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
               <% else %>
               <a href="#" onclick="$.get('<%= url_for :controller =>:Event, :action => :alert_show %>').success(function(){ console.log('Event'); }); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
               <% end %>
            </div>
            <% end %>
            <% if i===2 %>    
            <div class="ui-block-b">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type=> 1} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
            <% end %>
            <% if i===3 %>    
            <div class="ui-block-c">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 2} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
         </div>
         <% end %>      
         <% if i===4 %>  
         <div class="ui-grid-b middle">
            <div class="ui-block-a">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 3}  %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
            <% end %>
            <% if i===5 %>    
            <div class="ui-block-b">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 4} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
            <% end %>
            <% if i===6 %>    
            <div class="ui-block-c">
               <a href="/app/BarcodeRecognizerMoto/take">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
         </div>
         <% end %>      
         <% if i===7 %>  
         <div class="ui-grid-b bottom">
            <div class="ui-block-a">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 5} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
            <% end %>
            <% if i===8 %>    
            <div class="ui-block-b">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 6} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
            <% end %>
            <% if i===9 %>    
            <div class="ui-block-c">
               <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open_link, :query=> {:type => 7} %>').success(function(data){console.log('PDF')}); return false;">
               <img class="home icon" src="<%= Rho::RhoApplication::get_blob_path(event.icon_file) %>" />
               </a>
            </div>
         </div>
         <% end %>      
         <% end %>
      </div>
      <% end %>
   </div>
   <div class="ad">
      <% if $ad_image!="" %>
      <a href="#" onclick="$.get('<%= url_for :controller =>:Link, :action => :open, :query=> {:link => $website_link} %>').success(function(data){console.log('PDF')}); return false;"> 
      <img src="<%= Rho::RhoApplication::get_blob_path($ad_image) %>" />
      </a>
      <% end %>
   </div>
</div>