<h1>Listing icons</h1>

<table>
  <tr>
    <th>Icon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @icons.each do |icon| %>
  <tr>
    <td><%= icon.icon_name %></td>
    <td><%= link_to "View", icon.icon_file  %></td>
    <td><%= link_to 'Show', icon %></td>
    <td><%= link_to 'Edit', edit_icon_path(icon) %></td>
  </tr>
<% end %>
</table>

