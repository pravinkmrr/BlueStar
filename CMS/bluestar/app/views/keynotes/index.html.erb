<h1>Listing keynotes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Pdf</th>
    <th>Event</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @keynotes.each do |keynote| %>
  <tr>
    <td><%= keynote.name %></td>
    <td><%= link_to "View", keynote.filename %></td>
    <td><%= Event.find(keynote.event_id).name %></td>
    <td><%= link_to 'Show', keynote %></td>
    <td><%= link_to 'Edit', edit_keynote_path(keynote) %></td>
    <td><%= link_to 'Destroy', keynote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Keynote', new_keynote_path %>
